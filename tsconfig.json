{
  "compileOnSave": false,
  "compilerOptions": {
    "rootDir": "/",
    "baseUrl": "./",
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "declaration": false,
    "module": "es2020",
    "moduleResolution": "node",
    "experimentalDecorators": true,
    "target": "ES2022",
    "typeRoots": [
      "node_modules/@types"
    ],
    "lib": [
      "es2018",
      "dom"
    ],
    "paths": {
      "mat-multi-sort": [
        "dist/mat-multi-sort"
      ],
      "mat-multi-sort/*": [
        "dist/mat-multi-sort/*"
      ]
    },
    "useDefineForClassFields": false
  }
}
